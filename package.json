    "prettier": "^3.4.2",
    "puppeteer": "^24.21.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0",
    "workbox-window": "^7.3.0"
