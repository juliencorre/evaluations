    "test:unit": "npx vitest",
    "test:unit:run": "npx vitest run",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "lint": "npx eslint . --fix",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lighthouse": "npx lhci autorun",
    "test:ci": "node scripts/test-ci.js",
    "test:ci:fast": "node scripts/test-ci.js --skip-e2e --skip-lighthouse",
    "ci-fix": "node scripts/ci-auto-fix.js",
    "ci-check": "node scripts/test-ci.js",
    "lint-fix": "npx eslint . --fix",
    "type-fix": "npx vue-tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.49.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
